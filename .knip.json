{
    "$schema": "https://unpkg.com/knip@latest/schema.json",
    "entry": [
        "src/**/*.{ts,tsx,js,jsx}",
        "app/**/*.{ts,tsx,js,jsx}",
        ".eslintrc.js"
    ],
    "project": ["src/**/*.{ts,tsx,js,jsx}", "app/**/*.{ts,tsx,js,jsx}"],
    "ignoreDependencies": [
        "@chakra-ui/react",
        "@emotion/react",
        "@radix-ui/react-slot",
        "class-variance-authority",
        "clsx",
        "framer-motion",
        "postcss",
        "tailwind-merge",
        "tailwindcss",
        "tailwindcss-animate"
    ],
    "ignoreBinaries": [],
    "ignoreExportsUsedInFile": true,
    "next": {
        "entry": ["app/**/*.{ts,tsx,js,jsx}"]
    }
}
